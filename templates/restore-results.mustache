{{!
    @template local_coursecompletionrestore/restore-results

    This template renders the main content area for the dashboard block.

    Example context (json):
    {}
}}
<div id="snapshots-details">
    <h2>{{title}}</h2>
    <table class="results">
        <tr>
            <td>{{# str }} snapshot, local_coursecompletionrestore {{/ str }}:</td><td>{{snapshot}}</td>
        </tr>
        <tr>
            <td>{{# str }} course, local_coursecompletionrestore {{/ str }}:</td><td>{{course}}</td>
        </tr>
        <tr>
            <td>{{# str }} completions_restored, local_coursecompletionrestore {{/ str }}:</td><td>{{success_completions}}</td>
        </tr>
        <tr>
            <td>{{# str }} criteria_restored, local_coursecompletionrestore {{/ str }}:</td><td>{{success_criteria}}</td>
        </tr>
         <tr>
             <td>{{# str }} modules_restored, local_coursecompletionrestore {{/ str }}:</td><td>{{success_modules}}</td>
         </tr>
        <tr>
            <td>{{# str }} failed_completions, local_coursecompletionrestore {{/ str }}: </td><td>{{failed_completions}}</td>
        </tr>
        <tr>
            <td>{{# str }} failed_criteria, local_coursecompletionrestore {{/ str }}: </td><td>{{failed_criteria}}</td>
        </tr>
        <tr>
            <td>{{# str }} failed_modules, local_coursecompletionrestore {{/ str }}</td><td>{{failed_modules}}</td>
        </tr>
        <tr>
            <td>{{completions_array}}</td>
        </tr>
        <tr>
            <td>{{# str }} failed_criteria, local_coursecompletionrestore {{/ str }}</td>
        </tr>
        <tr>
            <td></td><td>{{criteria_array}}</td>
        </tr>
        <tr>
            <td>{{# str }} failed_modules, local_coursecompletionrestore {{/ str }}</td>
        </tr>
        <tr>
            <td></td><td>{{modules_array}}</td>
        </tr>
    </table>
</div>